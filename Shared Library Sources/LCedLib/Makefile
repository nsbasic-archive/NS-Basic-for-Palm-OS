CC = m68k-palmos-gccAS = m68k-palmos-asRC = pilrcCFLAGS = -Wall -O2 -DCOMPILING_LIBall: LCedLib.prc	cp LCedLib.prc /dl/LCedLib.prc: LCedLib.def LCedLib LCedLib.ro	build-prc -o $@ $^LCedLib: LCedLib.o LCedLib-dispatch.o	$(CC) -nostartfiles -o $@ $^LCedLib.o: LCedLib.c LCedLib.hclean:	rm -f *.o *.ro LCedLib%-dispatch.s: %.def	m68k-palmos-stubgen $^%.ro: %.res	$(RC) -ro -o $@ $^ |: