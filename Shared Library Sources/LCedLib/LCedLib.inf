[General]
ExtensionName=LCedLib
PrcName=LCedLib.prc
Version=1.0
Manufacturer=MvDuin
=HelpStart=
""
"Utility library for LCedit"
""
=HelpEnd=
[GlobalMethods]
Version=1, func, 0, "verDbl = Version()"
