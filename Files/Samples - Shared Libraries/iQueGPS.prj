"Sub Form1013_Event()
	Dim status as Integer
	Dim lat as Double
	Dim lon as Double
	Dim altMSL as Double
	Dim altWGS84 as Double
	Dim errString as String
	Dim latDegMin as String
	Dim lonDegMin as String
	Dim latDegMinSec as String
	Dim lonDegMinSec as String
	Dim LatSemi as Integer
	Dim LonSemi as Integer
	Dim zone as String
	Dim easting as Double
	Dim northing as Double
	Dim event as Integer
	
	event = GetEventType()
	
	If event = 30000 Then
    	status = NQG.GPSGetPosition(lat, lon, altMSL, altWGS84)
    	If status = 0 Then
    	    fldPosCount.text = Str(val(fldPosCount.text) + 1)
    	    NQG.GetLatLonSemicircles LatSemi, LonSemi
    	    NQG.GetLatLonDegMin latDegMin, LonDegMin
    	    NQG.GetLatLonDegMinSec latDegMinSec, LonDegMinSec
    	    fldLatDegMinSec.text = latDegMinSec
    	    fldLonDegMinSec.text = lonDegMinSec
    	    fldLatDegrees.text = Str(lat)
    	    fldLatDegMin.text = latDegMin
    	    fldLatSemi.text = Str(latSemi)
    	    fldLonDegrees.text = Str(lon)
    	    fldLonDegMin.text = lonDegMin
    	    fldLonSemi.text = Str(lonSemi)
    	    fldAltMSL.text = str(altMSL)
    	    fldAltWGS84.text = str(altWGS84)
    	    zone = NQG.LatLonToUTM(0, lat, lon, easting, northing)
    	    fldUTM.text = zone + ~  ~ + str(trunc(easting, 3)) + ~  ~ + str(trunc(northing, 3))
    	End If
    End If

End Sub"
