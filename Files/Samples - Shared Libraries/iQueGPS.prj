	Dim snr as Integer
	Dim azimuth as Double
	Dim elevation as Double
	Dim errString as String
	Dim eph as Integer
	Dim dif as Integer
	Dim used as Integer
	Dim rising as Integer
	
	If lstSatellites.selected <> 0 Then
	    satellite = val(lstSatellites.text(lstSatellites.selected))
	    status = NQG.GPSGetSatellites(satellite, svid, statusBits, snr, azimuth, elevation)
	    If status <> 0 Then
    	    errString = NQG.GetErrString(status)
	        MsgBox ~GPSGetSatellites error = ~ + errString
	    Else
	        fldSVID.text = str(svid)
	        fldStatusBits.text = str(statusBits)
	        NQG.GetSatStatusValues statusBits, eph, dif, used, rising
	        fldEphemeris.text = str(eph)
	        fldDifferential.text = str(dif)
	        fldUsed.text = str(used)
	        fldRising.text = str(rising)
	        fldSNR.text = str(snr)
	        fldAzimuth.text = str(azimuth)
	        fldElevation.text = str(elevation)
	    End If
	Else
	    MsgBox ~No satellite has been selected.~
    End If

End Sub"
