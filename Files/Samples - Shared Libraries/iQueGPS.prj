"but","Button1220",1220,4,144,12,68,#TRUE#,0,"UTMToLatLon"
"Sub object1220()
	Dim status as Integer
	Dim lat as Double
	Dim lon as Double
	Dim altMSL as Double
	Dim altWGS84 as Double
	Dim errString as String
	Dim zone as String
	Dim easting as Double
	Dim northing as Double
	
	status = NQG.GPSGetPosition(lat, lon, altMSL, altWGS84)
	If status <> 0 Then
	    errString = NQG.GetErrString(status)
	    MsgBox ~GPSGetPosition error = ~ + errString
	Else
	    fldOrigLat.text = str(lat)
	    fldOrigLon.text = str(lon)
	    zone = NQG.LatLonToUTM(0, lat, lon, easting, northing)
	    fldUTMx.text = zone + ~ ~ + str(trunc(easting, 3)) + ~ ~ + str(trunc(northing, 3))
	    lat = 0
	    lon = 0
	    NQG.UTMToLatLon 0, zone, easting, northing, lat, lon
	    fldNewLat.text = str(lat)
	    fldNewLon.text = str(lon)
	End If

End Sub"
